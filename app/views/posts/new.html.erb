<h2>New Post<h2>
    <%= form_with model: @posts do |form|%>
    <%= form.label :title, "title" %><br>
    <%= form.text_field :title, placeholder: "Enter your title here" %><br>
    <%= form.label :body, "body" %><br>
    <%= form.text_area :body, size: "70x5" %><br>
    <%= form.submit "submit"%>
    <%end%>